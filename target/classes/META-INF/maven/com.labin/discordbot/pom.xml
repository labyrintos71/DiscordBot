    <version>1.2.62</version>
  </dependency>
  
<!-- YouTube API -->
  <dependency>
      <groupId>com.google.apis</groupId>
      <artifactId>google-api-services-youtube</artifactId>
      <version>v3-rev193-1.23.0</version>
    </dependency>
    
<!-- OkHttp -->
  <dependency>
  	<groupId>com.squareup.okhttp3</groupId>
